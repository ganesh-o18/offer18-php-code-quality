#!/usr/bin/env php

<?php

if ($argc > 1) {
    // Loop through arguments (skip $argv[0] since itâ€™s the script name)
    for ($i = 1; $i < $argc; $i++) {
        echo "Argument $i: " . $argv[$i] . PHP_EOL;
    }
} else {
    echo "No arguments passed!" . PHP_EOL;
}
$code = file_get_contents('/github/workspace/dummy-test/index.php');

$tokens = token_get_all($code);

foreach ($tokens as $token) {
  if (is_array($token)) {
    echo token_name($token[0]) . ' ' . $token[1];
  }
}
// var_dump($tokens);
